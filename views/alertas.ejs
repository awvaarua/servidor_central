<div class="general">
  <table> <!-- bootstrap classes added by the uitheme widget -->
    <thead>
      <tr>
        <th>IP</th>
        <th>Tipo Sensor</th>
        <th>Condici√≥n</th>
        <th>Valor</th>
        <th>Mensjae</th>
        <th>Frecuencia</th>
        <th>Contactos</th>
        <th>Acciones</th>
    </thead>
    <tbody>
      <% alertas.forEach(function(alerta, i) { %>
        <tr id="alerta_<%= i %>">
          <td><%= alerta.ip %></td><td><%= alerta.tipo %></td><td><%= alerta.condicion %></td><td><%= alerta.valor %></td><td><%= alerta.mensaje %></td><td><%= alerta.frecuencia %><td><%= alerta.usuarios %></td>
          <td><button type="button" class="btn btn-danger btn-md" onclick="DeleteAlerta(<%= i %>, '<%= alerta.id %>');">
            <span class="glyphicon glyphicon-trash"></span> Eliminar</button></td>
          </tr>
      <% }); %>
    </tbody>
  </table>
</div>